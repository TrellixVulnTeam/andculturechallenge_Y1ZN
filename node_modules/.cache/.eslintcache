e/src/components/Home.js",[],"/Users/davidpark/Desktop/andculture/challenge/src/components/Navbar.js",[],"/Users/davidpark/Desktop/andculture/challenge/src/components/Add.js",[],"/Users/davidpark/Desktop/andculture/challenge/src/components/Edit.js",[],"/Users/davidpark/Desktop/andculture/challenge/src/components/Students.js",["60","61"],"import React, {Component} from 'react';\nimport {Link} from 'react-router-dom';\nimport axios from 'axios';\n\nconst Student = props => (\n  <tr>\n    <td>{props.student.first}</td>\n    <td>{props.student.last}</td>\n    <td>{props.student.email}</td>\n    <td>{props.student.age}</td>\n    <td>{props.student.grade}</td>\n    <td>\n        <Link to={\"/update/\"+props.student._id}>edit</Link> | <a href=\"#\" onClick={() => {if(window.confirm(\"Delete this item?\")) {{ props.deleteStudent(props.student._id) }}}} >delete</a>\n    </td>\n  </tr>\n);\n\nexport default class Home extends Component {\n  constructor(props) {\n    super(props);\n\n    this.deleteStudent = this.deleteStudent.bind(this);\n\n    this.state = {\n      students: [],\n      shownig: false\n    };\n  };\n\n  componentDidMount() {\n    axios.get('http://localhost:3001/students')\n      .then(res => {\n        this.setState({students: res.data})\n      })\n      .catch(err => {\n        console.log(err);\n      });\n  };\n\n  deleteStudent(id) {\n    axios.delete('http://localhost:3001/students/'+id)\n      .then(res => console.log(res.data))\n      .catch(err => console.log(err));\n\n    this.setState({\n      students: this.state.students.filter(el => el._id !== id)\n    })\n\n  };\n\n  studentsList() {\n    return this.state.students.map(currentStudent => {\n      return <Student student={currentStudent} deleteStudent={this.deleteStudent} key={currentStudent._id}/>;\n    })\n  }\n\n  render() {\n    return (\n      <div className=\"container\">\n          <div className=\"panel-heading\">\n            <h3 className=\"panel-title\">Student List</h3>\n          </div>      \n          <table className=\"table\">\n          <thead className=\"thead-dark\">\n            <tr>\n              <th>First Name</th>\n              <th>Last Name</th>\n              <th>Email</th>\n              <th>Age</th>\n              <th>Grade</th>\n              <th>Actions</th>\n            </tr>\n          </thead>\n          <tbody>\n            { this.studentsList() }\n          </tbody>\n        </table>\n      </div>\n    );\n  }\n}\n","/Users/davidpark/Desktop/challenge/src/index.js",[],"/Users/davidpark/Desktop/challenge/src/App.js",[],"/Users/davidpark/Desktop/challenge/src/components/Home.js",[],"/Users/davidpark/Desktop/challenge/src/components/Students.js",["62","63"],"/Users/davidpark/Desktop/challenge/src/components/Edit.js",[],"/Users/davidpark/Desktop/challenge/src/components/Navbar.js",[],"/Users/davidpark/Desktop/challenge/src/components/Add.js",[],{"ruleId":"64","severity":1,"message":"65","line":13,"column":63,"nodeType":"66","endLine":13,"endColumn":179},{"ruleId":"67","severity":1,"message":"68","line":13,"column":132,"nodeType":"69","messageId":"70","endLine":13,"endColumn":174},{"ruleId":"64","severity":1,"message":"65","line":13,"column":63,"nodeType":"66","endLine":13,"endColumn":179},{"ruleId":"67","severity":1,"message":"68","line":13,"column":132,"nodeType":"69","messageId":"70","endLine":13,"endColumn":174},"jsx-a11y/anchor-is-valid","The href attribute requires a valid value to be accessible. Provide a valid, navigable address as the href value. If you cannot provide a valid href, but still need the element to resemble a link, use a button and change it with appropriate styles. Learn more: https://github.com/evcohen/eslint-plugin-jsx-a11y/blob/master/docs/rules/anchor-is-valid.md","JSXOpeningElement","no-lone-blocks","Nested block is redundant.","BlockStatement","redundantNestedBlock"]